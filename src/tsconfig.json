{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"isolatedModules": false,
		"declaration": true,
		"removeComments": false,
		"noLib": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"outDir": "../dist/src/",
		"types": ["node"],
		"lib": ["dom", "es6", "es2015.promise", "es2018.promise"]
	}
}
